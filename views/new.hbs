
  <form method="POST" action="/new">
  <h3>Did you find a new Wifi?</h3>
    <h3>Help Users: Point it on the map and add the new Wifi to our Data Base</h3>
    <input  id="new-input"type="text" name="name" placeholder="name" required>
      
    <input type="hidden" id="lat-pos"  name="latitude" required>
    <input type="hidden" id="lng-pos"  name="longitude" required>
    <input class="btn btn-primary button-new" type="submit" value="Add Wifi">
  </form>
  {{#if error}}
  <p class="new-error">{{error}}<i class="small material-icons">backspace</i></p>
{{/if}}
  <div id="map-user"></div>
</div>
<script src="/javascripts/map/markers.js"></script>

